    function loadCodeBackground() {
        fetch("/codeBackground").then((data)=>data.text())
        .then((data)=>{
            document.getElementById("codeForeground").innerText=data
            document.getElementById("codeBackground").innerText=data
        })
    }
    loadCodeBackground();
</script>
